{% if items.message %}
  <p>No fue posible acceder a la informacion solicitada. Por favor, reintente en unos instantes.</p>
{% endif %}
{% if items.trackError %}
  <table class="c-table c-table--basic c-table--responsive">

    <thead class="c-table__head">
    <tr>
      <th class="u-text_center">Fondo</th>
      <th class="u-text_center">Serie</th>
      <th class="u-text_center">Error de Seguimiento</th>
      <th class="u-text_center">Diferencia de Rentabilidad Acumulada</th>
    </tr>
    </thead>
    <tbody class="c-table__body">
    {% for item in items.trackError %}
      <tr>
        <td class="u-text_center">{{ item.fondo }}</td>
        <td class="u-text_center">{{ item.serie }}</td>
        <td class="u-text_center">{{ item.porcentajeError }} %</td>
        <td class="u-text_center">{{ item.porcentajeDiferenciaAcumulada }} %</td>
      </tr>
    {% endfor %}
    </tbody>
  </table>
{% endif %}

